a = int(input())
b = int(input())
num1 = a*b
print(a, 'умноженное на', b, 'равно', num1)
num2 = a/b
print(a, 'деленное на', b, 'равно', num2)
num3 = a//b
print(a, 'нацело деленное на', b, 'равно', num3)
num4 = a % b
print('Остаток от деления', a, 'на', b, 'равен', num4)
num5 = a**b
print(a, 'в степени', b, 'равно', num5)
